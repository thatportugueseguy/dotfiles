# Android dev environment variables
export ANDROID_HOME=$HOME/Library/Android/sdk
export PATH=$PATH:$ANDROID_HOME/tools
export PATH=$PATH:$ANDROID_HOME/platform-tools

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/ze/Projects/playground/google-cloud-platform/nodejs-docs-samples/y/google-cloud-sdk/path.zsh.inc' ]; then source '/Users/ze/Projects/playground/google-cloud-platform/nodejs-docs-samples/y/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/ze/Projects/playground/google-cloud-platform/nodejs-docs-samples/y/google-cloud-sdk/completion.zsh.inc' ]; then source '/Users/ze/Projects/playground/google-cloud-platform/nodejs-docs-samples/y/google-cloud-sdk/completion.zsh.inc'; fi
